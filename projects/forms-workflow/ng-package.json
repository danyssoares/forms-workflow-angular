{
  "$schema": "../../node_modules/ng-packagr/ng-package.schema.json",
  "dest": "../../dist/forms-workflow",
  "assets": [
    {
      "glob": "**/*.scss",
      "input": "src/lib",
      "output": "lib"
    },
    {
      "glob": "**/*",
      "input": "src/assets",
      "output": "assets"
    }
  ],
  "lib": {
    "entryFile": "src/public-api.ts"
  }
}
